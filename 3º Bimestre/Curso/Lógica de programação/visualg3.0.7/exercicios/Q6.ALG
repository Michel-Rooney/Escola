Algoritmo "Q6"

Var
   n1, n2 : Caracter
   p1, p2 : Real


Inicio
      escreva("Nome 1: ")
      leia(n1)
      escreva("Peso 1: ")
      leia(p1)
      escreva("Nome 2: ")
      leia(n2)
      escreva("Peso 2: ")
      leia(p2)
      
      escreva("Primeira pessoa: ", n1, " ", p1)
      escreva(" Primeira pessoa: ", n2, " ", p2)


Fimalgoritmo