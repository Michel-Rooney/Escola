Algoritmo "Q5"

Var
   nome : Caracter
   idade : Inteiro
   peso : Real


Inicio
      escreva("Digite seu nome: ")
      leia(nome)
      escreva("Digite sua idade: ")
      leia(idade)
      escreva("Digite seu peso: ")
      leia(peso)
      escreva("Seu nome é ",nome, " Sua idade é", idade, " Seu peso é ", peso)
      


Fimalgoritmo