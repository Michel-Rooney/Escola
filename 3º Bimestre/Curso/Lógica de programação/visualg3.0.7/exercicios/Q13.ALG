Algoritmo "Q13"
Var
   n1, n2, n3, soma, sub : inteiro

Inicio
      escreva("Num1: ")
      leia(n1)
      escreva("Num2: ")
      leia(n2)
      escreva("Num3: ")
      leia(n3)
      soma <- (n1+n2)
      sub <- (n3 - soma)
      escreva(" Num digitadotos", n1, n2, n3)
      escreva(" Soma: ", soma)
      escreva(" Sub: ", sub)

Fimalgoritmo