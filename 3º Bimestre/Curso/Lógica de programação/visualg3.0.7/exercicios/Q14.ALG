Algoritmo "Q14"
Var
   n1, n2, n3, mult, soma : inteiro

Inicio
      escreva("Num1: ")
      leia(n1)
      escreva("Num2: ")
      leia(n2)
      escreva("Num3: ")
      leia(n3)
      mult <- (n1*n2)
      soma <- (mult + n3)
      
      escreva(" Numeros usados: ", n1, n2, n3)
      escreva(" Mult: ", mult)
      escreva(" Soma: ", soma)

Fimalgoritmo