Algoritmo "Q10"
Var
    p1, p2, p3, p4, me_geo : real


Inicio
      escreva("Peso1: ")
      leia(p1)
      escreva("Peso2: ")
      leia(p2)
      escreva("Peso3: ")
      leia(p3)
      escreva("Peso4: ")
      leia(p4)
      me_geo <- (p1*p2*p3*p4)/4
      escreva("A media geometria é", me_geo)


Fimalgoritmo